#include <stdio.h>
#include <string.h>

int main() {
    FILE *fp = fopen("sample.txt", "r");
    char line[200], keyword[50];
    printf("Enter keyword: ");
    scanf("%s", keyword);
    while (fgets(line, sizeof(line), fp)) {
        if (strstr(line, keyword))
            printf("%s", line);
    }
    fclose(fp);
    return 0;
}
